@model SuministradorModel

@{
    ViewData["Title"] = "Crear Suministrador";
}

<div class="container">

    <form method="post">
        <div class="col-lg-3">
            <label>Razón Social</label>
            <input class="form-control" asp-for="RazonSocial"/>
        </div>

        <div class="col-lg-3">
            <label>NIF</label>
            <input class="form-control" asp-for="CIF" />
        </div>

        <div class="col-lg-3">
            <label>Descripcion</label>
            <input class="form-control" asp-for="Descripcion" />
        </div>

        <div class="col-lg-3">
            <label>Fecha de Creación</label>
            <input class="form-control" type="datetime"/>
        </div>

        <div class="col-lg-3">
            <label>Correo Electrónico</label>
            <input class="form-control" type="email" />
        </div>

        <div class="col-lg-3">
            <label>Telefono</label>
            <input class="form-control" />
        </div>

    </form>
</div>